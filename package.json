{
  "name": "readonly-types",
  "version": "2.0.0",
  "description": "A collection of readonly TypeScript types inspired by the built-in ReadonlyArray, ReadonlyMap, etc.",
  "main": "dist",
  "repository": "https://github.com/danielnixon/readonly-types.git",
  "author": "Daniel Nixon <dan.nixon@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "codecov": "^3.8.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-typed-fp": "^1.5.0",
    "eslint-plugin-functional": "^3.7.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-total-functions": "^4.9.0",
    "jest": "^27.1.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "type-coverage": "^2.18.1",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "type-coverage": "type-coverage --strict --detail",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "release": "yarn build && yarn lint && yarn type-coverage && yarn publish"
  },
  "typeCoverage": {
    "atLeast": 97.08
