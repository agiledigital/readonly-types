    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-spellcheck": "^0.0.20",
    "eslint-plugin-total-functions": "^6.0.0",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "type-coverage": "^2.24.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives",
