    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "type-coverage": "^2.26.2",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives",
