{
  "name": "readonly-types",
  "version": "3.0.0",
  "description": "A collection of readonly TypeScript types inspired by the built-in ReadonlyArray, ReadonlyMap, etc.",
  "main": "dist",
  "repository": "https://github.com/danielnixon/readonly-types.git",
  "author": "Daniel Nixon <dan.nixon@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@stryker-mutator/core": "6.1.2",
    "@stryker-mutator/jest-runner": "6.1.2",
    "@stryker-mutator/typescript-checker": "6.1.2",
    "@types/jest": "^28.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "codecov": "^3.8.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-typed-fp": "^2.0.1",
    "eslint-plugin-functional": "^4.0.2",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-total-functions": "^6.0.0",
    "jest": "^28.0.0",
    "prettier": "^2.5.1",
    "ts-jest": "^28.0.0",
    "type-coverage": "^2.19.0",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "release": "yarn build && yarn lint && yarn type-coverage && yarn publish"
  },
  "typeCoverage": {
    "atLeast": 98.6,
    "ignoreCatch": false,
    "strict": true,
    "detail": true
  }
}
