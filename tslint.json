{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-immutable/all",
        "tslint-sonarts",
        "tslint-eslint-rules",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "no-any": true,
        "no-unsafe-any": true,
        "strict-boolean-expressions": true,
        "interface-over-type-literal": false,
        "interface-name": false,
        "member-ordering": false
    },
    "rulesDirectory": []
}